{
    "extends": "airbnb",
    "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "parser": "babel-eslint",
    "rules": {
      "no-console": "off",
      "strict": ["error", "global"],
      "curly": "warn",
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "jsx-a11y/label-has-for": [ 2, {
        "required": {
            "some": [ "nesting", "id" ]
        }
      }],
      "no-param-reassign": 0
    }
  }